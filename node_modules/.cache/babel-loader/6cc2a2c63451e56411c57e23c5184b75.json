{"ast":null,"code":"import { Edit } from \"@element-plus/icons-vue\";\nimport { ElMessage } from \"element-plus\";\nimport { acceptTask } from \"@/api/task\";\nimport { employeeTaskDetail } from \"@/api/square\";\nimport { employerTaskDetail } from \"@/api/square\";\nexport default {\n  name: \"TaskInfoFromSquare\",\n\n  data() {\n    return {\n      taskId: this.$route.params.taskId,\n      task: {// taskId: 0,\n        // taskName: 'test_task',\n        // taskType: 0,\n        // workerNumTotal: 10,\n        // workerNumLeft: 5,\n        // taskStartTime: '2022-3-1',\n        // taskEndTime: '2022-5-3',\n        // taskState: true,\n        // taskIntroduction: '这是一个测试任务',\n      },\n      role: window.localStorage.getItem(\"role\"),\n      isSelected: false,\n      isAble: this.taskState || this.role === '1' || this.isSelected\n    };\n  },\n\n  mounted() {\n    if (this.role === '1') {\n      employeeTaskDetail({\n        token: window.localStorage.getItem(\"token\"),\n        taskId: this.taskId\n      }).then(res => {\n        if (res.response.code === 0) {\n          this.task = res.task;\n          this.isSelected = res.isSelected;\n        }\n      });\n    } else if (this.role === '0') {\n      employerTaskDetail({\n        token: window.localStorage.getItem(\"token\"),\n        taskId: this.taskId\n      }).then(res => {\n        if (res.response.code === 0) {\n          console.log(res.response.msg);\n          this.task = res.task;\n        } else {\n          console.log(res.response.msg);\n        }\n      });\n    }\n  },\n\n  components: {\n    Edit\n  },\n  methods: {\n    enroll() {\n      const token = window.localStorage.getItem(\"token\");\n      acceptTask({\n        token: token,\n        taskId: this.taskId\n      }).then(res => {\n        if (res.response.ode === 0) {\n          this.$router.push(\"/taskEnrollSucceed\");\n          console.log(res.response.msg);\n          console.log(res.data);\n        } else {\n          console.log(res.response.msg);\n        }\n      });\n    },\n\n    showEnrollError() {\n      ElMessage.error('非常抱歉，报名已经结束，看看其他项目吧');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyCA,SAAQA,IAAR,QAAmB,yBAAnB;AACA,SAAQC,SAAR,QAAwB,cAAxB;AACA,SAAQC,UAAR,QAAyB,YAAzB;AACA,SAAQC,kBAAR,QAAiC,cAAjC;AACA,SAAQC,kBAAR,QAAiC,cAAjC;AACA,eAAe;AACbC,MAAI,EAAE,oBADO;;AAEbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,MADtB;AAELG,UAAI,EAAE,CACJ;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AATI,OAFD;AAaLC,UAAI,EAACC,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,MAA5B,CAbA;AAcLC,gBAAU,EAAE,KAdP;AAeLC,YAAM,EAAE,KAAKC,SAAL,IAAkB,KAAKN,IAAL,KAAc,GAAhC,IAAuC,KAAKI;AAf/C,KAAP;AAiBD,GApBY;;AAqBbG,SAAO,GAAG;AACR,QAAG,KAAKP,IAAL,KAAY,GAAf,EACA;AACER,wBAAkB,CAAC;AAACgB,aAAK,EAACP,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,OAA5B,CAAP;AAA4CP,cAAM,EAAC,KAAKA;AAAxD,OAAD,CAAlB,CACCa,IADD,CACMC,GAAE,IAAK;AACX,YAAGA,GAAG,CAACC,QAAJ,CAAaC,IAAb,KAAsB,CAAzB,EACA;AACE,eAAKb,IAAL,GAAYW,GAAG,CAACX,IAAhB;AACA,eAAKK,UAAL,GAAkBM,GAAG,CAACN,UAAtB;AACF;AACD,OAPD;AAQF,KAVA,MAWK,IAAG,KAAKJ,IAAL,KAAc,GAAjB,EACL;AACEP,wBAAkB,CAAC;AAACe,aAAK,EAACP,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,OAA5B,CAAP;AAA4CP,cAAM,EAAC,KAAKA;AAAxD,OAAD,CAAlB,CACCa,IADD,CACMC,GAAE,IAAK;AACX,YAAGA,GAAG,CAACC,QAAJ,CAAaC,IAAb,KAAsB,CAAzB,EACA;AACEC,iBAAO,CAACC,GAAR,CAAYJ,GAAG,CAACC,QAAJ,CAAaI,GAAzB;AACA,eAAKhB,IAAL,GAAYW,GAAG,CAACX,IAAhB;AACF,SAJA,MAKK;AACHc,iBAAO,CAACC,GAAR,CAAYJ,GAAG,CAACC,QAAJ,CAAaI,GAAzB;AACF;AACD,OAVD;AAWF;AACD,GA/CY;;AAgDbC,YAAU,EAAE;AACV3B;AADU,GAhDC;AAmDb4B,SAAO,EAAE;AACPC,UAAM,GAAE;AACN,YAAMV,KAAI,GAAIP,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,OAA5B,CAAd;AACAZ,gBAAU,CAAC;AAACiB,aAAK,EAACA,KAAP;AAAaZ,cAAM,EAAC,KAAKA;AAAzB,OAAD,CAAV,CACCa,IADD,CACMC,GAAE,IAAK;AACX,YAAGA,GAAG,CAACC,QAAJ,CAAaQ,GAAb,KAAqB,CAAxB,EAA0B;AACxB,eAAKC,OAAL,CAAaC,IAAb,CAAkB,oBAAlB;AACAR,iBAAO,CAACC,GAAR,CAAYJ,GAAG,CAACC,QAAJ,CAAaI,GAAzB;AACAF,iBAAO,CAACC,GAAR,CAAYJ,GAAG,CAACf,IAAhB;AACF,SAJA,MAKK;AACHkB,iBAAO,CAACC,GAAR,CAAYJ,GAAG,CAACC,QAAJ,CAAaI,GAAzB;AACF;AACD,OA